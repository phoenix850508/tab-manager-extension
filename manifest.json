{
  "manifest_version": 3,
  "name": "Tab Manager Extension",
  "version": "1.0",
  "icons": {
    "16": "images/content_list.png",
    "32": "images/content_list.png",
    "48": "images/content_list.png",
    "128": "images/content_list.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage"]
}